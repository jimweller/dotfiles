<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.user.awsrefreshtoken</string>

    <key>ProgramArguments</key>
    <array>
      <string>/Users/jimweller/.config/dotfiles/scripts/aws-refresh-token.sh</string>
    </array>

    <key>RunAtLoad</key>
    <true />

    <!-- Runs at the zero minute of every hour-->
    <key>StartCalendarInterval</key>
    <dict>
      <key>Minute</key>
      <integer>0</integer>
    </dict>

    <key>StandardOutPath</key>
    <string>/Users/jimweller/assets/aws/launchd.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/jimweller/assets/aws/launchd.err</string>
  </dict>
</plist>